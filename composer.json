{
    "name": "wizad/settings-bundle",
    "description": "Settings system for Symfony2 applications, editable via web user interface, injected in service container.",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "William POTTIER",
            "email": "developer@william-pottier.fr"
        }
    ],
    "require": {
        "php":                          ">=5.5.9",
        "symfony/framework-bundle":     "^2.7|^3.0",
        "symfony/twig-bundle":          "^2.7|^3.0",
        "symfony/form":                 "^2.7|^3.0",
        "symfony/validator":            "^2.7|^3.0",
        "symfony/config":               "^2.7|^3.0",
        "symfony/yaml":                 "^2.7|^3.0",
        "predis/predis":                "v0.8.3"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {"Wizad\\SettingsBundle\\": ""},
        "exclude-from-classmap": ["/spec/"]
    },
    "target-dir": "Wizad//SettingsBundle"
}
