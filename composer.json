{
    "name": "wizad/settings-bundle",
    "description": "Settings system for Symfony2 applications, editable via web user interface, injected in service container.",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "William POTTIER",
            "email": "developer@william-pottier.fr"
        }
    ],
    "require": {
        "php":                          ">=5.3.3",
        "symfony/framework-bundle":     "~2.3",
        "symfony/twig-bundle":          "~2.1",
        "symfony/form":                 "~2.3",
        "symfony/validator":            "~2.3",
        "symfony/config":               "~2.3",
        "predis/predis":                "v0.8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "symfony/css-selector": "~2.1",
        "symfony/browser-kit": "~2.1",
        "symfony/validator": "~2.1",
        "symfony/yaml": "~2.1",
        "friendsofsymfony/rest-bundle": "0.12.*@dev",
        "sensio/framework-extra-bundle": "~2.1",
        "symfony/expression-language":  "~2.3"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Wizad\\SettingsBundle": "" }
    },
    "target-dir": "Wizad//SettingsBundle"
}
